{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "no-consecutive-blank-lines": [
          false
        ],
        "no-unused-expression": [
          true,
          "allow-new"
        ],
        "ordered-imports": [
          false
        ],
        "quotemark": [
          true,
          "single"
        ],
        "indent": [
          true
        ],
        "interface-name": [
          false
        ],
        "arrow-parens": false,
        "object-literal-sort-keys": false,
        "prettier": true
    },
  "rulesDirectory": [
      "tslint-plugin-prettier"
  ]
}

